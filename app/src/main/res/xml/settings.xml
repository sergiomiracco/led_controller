<?xml version="1.0" encoding="utf-8"?>

<androidx.preference.PreferenceScreen
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
        app:title="Modules">

        <CheckBoxPreference
            app:key="shutter"
            app:title="Shutter"
            app:defaultValue="true"
            app:summaryOn="Enabled"
            app:summaryOff="Disabled"
        />

        <CheckBoxPreference
                app:key="rainbow"
                app:title="Rainbow"
                app:defaultValue="true"
                app:summaryOn="Enabled"
                app:summaryOff="Disabled"
        />

    </PreferenceCategory>

    <PreferenceCategory
        app:title="Peripherial">

        <ListPreference
            app:key="Devices"
            app:title="Device">

        </ListPreference>

        <ListPreference
                app:key="Protocols"
                app:title="Protocol">

        </ListPreference>


    </PreferenceCategory>

</androidx.preference.PreferenceScreen>
